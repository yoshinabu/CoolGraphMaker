        MajorOfXAxis="10" MinorOfXAxis="0"
        MajorOfYAxis="5" MinorOfYAxis="1">
    <DataLine Index="1" XData="Yes" FirstDataRow="5" StartDataColumn="3" EndDataColumn="69" Color="" />
